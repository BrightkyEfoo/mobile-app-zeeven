{
  "expo": {
    "name": "Zeeven",
    "slug": "mobile-app-zeeven",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/zeeven.png",
      "resizeMode": "contain",
      "backgroundColor": "#041d3f"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Autorisez ZEEVEN à accéder à votre caméra",
          "photosPermission": "Autorisez ZEEVEN à accéder à votre appareil photo"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Autorisez ZEEVEN à accéder à votre caméra",
          "photosPermission": "Autorisez ZEEVEN à accéder à votre appareil photo"

        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "chillo.zeeven",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
